{
   "exclude": [
      "node_modules/**/*",
      "**/*.spec.ts"
   ],
   "compilerOptions": {
      "target": "es2016",



      "lib": ["es2016", "esnext.asynciterable"],
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,

      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,

      "forceConsistentCasingInFileNames": true,
      "baseUrl": ".",
      "moduleResolution": "node",
      "paths": {
         "*": ["src/*"]
      }
   }
}